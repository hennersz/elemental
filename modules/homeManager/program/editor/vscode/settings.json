{
    "editor.tabSize": 2,
    "editor.fontFamily": "'SpaceMono Nerd Font', SpaceMono, Menlo, Monaco, 'Courier New', monospace",
    "workbench.colorTheme": "Solarized Light",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.fontFamily": "SpaceMono Nerd Font",
    "explorer.confirmDelete": false,
    "vsicons.dontShowNewVersionMessage": true,
    "explorer.confirmDragAndDrop": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "prettier.eslintIntegration": true,
    "prettier.printWidth": 120,
    "prettier.singleQuote": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.confirmSync": false,
    "javascript.validate.enable": false,
    "editor.acceptSuggestionOnEnter": "off",
    "yaml.schemaStore.enable": false,
    "go.testOnSave": true,
    "go.coverOnSave": true,
    "go.coverageDecorator": {
        "uncoveredHighlightColor": "rgba(211,54,130,0.20)",
        "coveredHighlightColor": "rgba(253,246,227,0.0)",
        "type": "highlight"
    },
    "go.autocompleteUnimportedPackages": true,
    "go.lintTool": "golangci-lint",
    "go.buildOnSave": "workspace",
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.vetOnSave": "off",
    "go.testFlags": [
        "-v"
    ],
    "go.lintFlags": [
        "--fast"
    ],
    "go.lintOnSave": "file",
    "terminal.integrated.shell.osx": "/usr/local/bin/fish",
    "go.coverageOptions": "showUncoveredCodeOnly",
    "go.gopath": "",
    "remote.SSH.remotePlatform": {
        "vm": "linux"
    },
    "shellcheck.customArgs": [
        "-x"
    ],
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontSize": 13,
    "editor.fontSize": 13,
    "redhat.telemetry.enabled": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "yaml.customTags": [
        "!encrypted/pkcs1-oaep scalar",
        "!vault scalar"
    ],
    "cSpell.userWords": [
        "automations",
        "containerised",
        "crossplane",
        "Distroless",
        "hennersz",
        "kyverno",
        "minimisation",
        "organisations",
        "Organised",
        "OSCAL",
        "proxied",
        "Quickstart",
        "unauthorised"
    ],
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "go.toolsManagement.autoUpdate": true,
    "hadolint.hadolintPath": "/home/henry/.nix-profile/bin/hadolint",
    "editor.largeFileOptimizations": false,
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "window.zoomLevel": 1,
    "remote.SSH.useLocalServer": false
}
